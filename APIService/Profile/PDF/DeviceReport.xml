<?xml version="1.0" encoding="utf-8" ?>
<file>
  <margin top="1.5" bottom="1.5" left="1.5" right="1.5"></margin>
  <report>
    <title>異常設備一覽</title>
    <data spacing-before="10" font-size="10">
      <data-header>
        <table-row font-size="8">
          <table-cell width="13" align="center">設備名稱</table-cell>
          <table-cell width="13" align="center">設備IP</table-cell>
          <table-cell width="10" align="center">管理者</table-cell>
          <table-cell width="16" align="center">異常時間</table-cell>
          <table-cell width="16" align="center">異常資訊</table-cell>
          <table-cell width="6" align="center">狀態</table-cell>
          <table-cell width="10" align="center">修復人員</table-cell>
          <table-cell width="16" align="center">廠商資訊</table-cell>
        </table-row>
      </data-header>
      <data-body font-size="6">
        <data-column width="13" align="center">#[DEVICE_NAME]#</data-column>
        <data-column width="13" align="center">#[DEVICE_ID]#</data-column>
        <data-column width="10" align="center">#[MAINTAINER_DISPLAY]#</data-column>
        <data-column width="16" align="center">#[DEVICE_LOG.LOG_INFO.ERROR_TIME]#</data-column>
        <data-column width="16" align="center">#[DEVICE_LOG.LOG_INFO.ERROR_INFO]#</data-column>
        <data-column width="6" align="center">#[STATUS_INFO.CL_DESC]#</data-column>
        <data-column width="10" align="center">#[REPAIRMAN_INFO.USER_NAME]#</data-column>
        <data-column width="16">#[SUPPLIER_DISPLAY]#</data-column>
      </data-body>
    </data>
  </report>
</file>