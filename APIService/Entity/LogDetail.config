<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entities" type="SourceHelper.Configuration.EntitySection, SourceHelper.Configuration" />
  </configSections>
  <entities>
    <list>
      <!--事件紀錄詳細資料-->
      <entity type="LogDetail">
        <table schema="dms" name="TB_LOG_INFO"></table>
        <view schema="dms" name="VW_LOG_INFO"></view>
        <properties>
          <property name="LOG_SN" schemaType="Int" isKey="true" />
          <property name="DEVICE_SN" schemaType="VarChar" schemaSize="7" />
          <property name="DEVICE_ID" schemaType="VarChar" schemaSize="40" />
          <property name="DEVICE_NAME" schemaType="NVarChar" schemaSize="30" />
          <property name="DEVICE_STATUS" schemaType="VarChar" schemaSize="1" />
          <property name="ERROR_TIME" schemaType="DateTime" />
          <property name="ERROR_INFO" schemaType="NVarChar" schemaSize="100" />
          <property name="USERID" schemaType="VarChar" schemaSize="30" />
          <property name="USER_NAME" schemaType="NVarChar" schemaSize="200" />
          <property name="REPAIR_TIME" schemaType="DateTime" />
          <property name="REPAIR_INFO" schemaType="NVarChar" schemaSize="100" />
          <property name="UP_TIME" schemaType="DateTime" />
        </properties>
        <relations>
          <relation propertyName="GROUP_LIST" relationEntity="DeviceGroup" relationType="LEFT JOIN" isCollection="true">
            <keys>
              <key source="DEVICE_SN" target="DEVICE_SN"></key>
            </keys>
          </relation>
          <relation propertyName="MAINTAINER_LIST" relationEntity="DeviceMaintainer" relationType="LEFT JOIN" isCollection="true">
            <keys>
              <key source="DEVICE_SN" target="DEVICE_SN"></key>
            </keys>
          </relation>
        </relations>
        <relationPlan>
          <plan name="Detail">
            <list>
              <add propertyName="GROUP_LIST"></add>
              <add propertyName="MAINTAINER_LIST"></add>
            </list>
          </plan>
        </relationPlan>
      </entity>
    </list>
  </entities>
</configuration>