<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entities" type="SourceHelper.Configuration.EntitySection, SourceHelper.Configuration" />
  </configSections>
  <entities>
    <list>
      <!--告警條件-->
      <entity type="AlarmCondition">
        <table schema="dms" name="TB_ALARM_CONDITION"></table>
        <properties>
          <property name="DEVICE_SN" schemaType="VarChar" schemaSize="7" isKey="true"></property>
          <property name="TARGET_NAME" schemaType="VarChar" schemaSize="128" isKey="true"></property>
          <property name="TARGET_VALUE" schemaType="VarChar" schemaSize="256"></property>
          <property name="IS_EXCEPTION" schemaType="VarChar" schemaSize="1"></property>
        </properties>
        <storeProcedure>
          <procedure schema="dms" name="SPC_ALARM_TARGET" type="Insert" group="SPC_ALARM_TARGET"></procedure>
        </storeProcedure>
        <parameterGroup>
          <group name="SPC_ALARM_TARGET">
            <map>
              <parameter name="DEVICE_SN"></parameter>
              <parameter name="TARGET_NAME"></parameter>
              <parameter name="TARGET_VALUE"></parameter>
              <parameter name="IS_EXCEPTION"></parameter>
            </map>
          </group>
        </parameterGroup>
      </entity>
    </list>
  </entities>
</configuration>