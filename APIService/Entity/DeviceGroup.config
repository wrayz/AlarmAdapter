<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entities" type="SourceHelper.Configuration.EntitySection, SourceHelper.Configuration" />
  </configSections>
  <entities>
    <list>
      <!--設備群組資料-->
      <entity type="DeviceGroup">
        <table schema="dms" name="TB_DEVICE_INFO"></table>
        <view schema="dms" name="VW_DEVICE_GROUP"></view>
        <properties>
          <property name="DEVICE_SN" schemaType="VarChar" schemaSize="7" isKey="true" />
          <property name="GROUP_SN" schemaType="VarChar" schemaSize="3" isKey="true" />
          <property name="GROUP_NAME" schemaType="Varchar" schemaSize="30" />
        </properties>
      </entity>
    </list>
  </entities>
</configuration>